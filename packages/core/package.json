		"build": "tsc --project tsconfig.build.json"
	},
	"dependencies": {
		"@hono/node-server": "1.18.0",
		"fuse.js": "^7.1.0",
		"hono": "4.8.10"
	},
	"devDependencies": {
		"@hono/vite-build": "1.6.2",
